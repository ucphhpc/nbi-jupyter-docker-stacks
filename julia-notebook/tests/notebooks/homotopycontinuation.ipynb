   "source": [
    "# declare variables x and y\n",
    "@var x y\n",
    "# define the polynomials\n",
    "f₁ = (x^4 + y^4 - 1) * (x^2 + y^2 - 2) + x^5 * y\n",
    "f₂ = x^2+2x*y^2 - 2y^2 - 1/2\n",
    "F = System([f₁, f₂])\n",
    "result = solve(F)"
   ]
